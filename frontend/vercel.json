{
  "routes": [
    {
      "src": "/sitemap-index.xml",
      "dest": "https://rate-my-student-life-backend.onrender.com/sitemap-index.xml",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=86400",
        "Content-Type": "application/xml",
        "X-Robots-Tag": "noindex"
      }
    },
    {
      "src": "/sitemap-universities-(.*).xml",
      "dest": "https://rate-my-student-life-backend.onrender.com/sitemap-universities-$1.xml",
      "headers": {
        "Cache-Control": "public, max-age=86400, s-maxage=86400",
        "Content-Type": "application/xml",
        "X-Robots-Tag": "noindex"
      }
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css; charset=utf-8"
        }
      ]
    },
    {
      "source": "/static/(.*.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    }
  ]
}