{
  "routes": [
    {
      "src": "/sitemap-index.xml",
      "dest": "/sitemap-index.xml",
      "headers": {
        "Content-Type": "application/xml",
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/sitemap-universities-(.*).xml",
      "dest": "/sitemap-universities-$1.xml",
      "headers": {
        "Content-Type": "application/xml",
        "Cache-Control": "public, max-age=86400"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}